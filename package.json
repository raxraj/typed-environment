{
    "name": "typed-environment",
    "version": "0.1.5",
    "description": "typed-environment is a zero-dependency, TypeScript-native library to load and validate environment variables using a custom schema. Get fully typed, safe, and reliable config objects with ease.",
    "license": "MIT",
    "author": "Ashutosh Kumar",
    "type": "commonjs",
    "main": "dist/index.js",
    "scripts": {
        "test": "jest",
        "build": "tsc",
        "prettier": "prettier --write src/**/*.ts",
        "prepublishOnly": "npm run build",
        "lint": "gts lint",
        "clean": "gts clean",
        "compile": "tsc",
        "fix": "gts fix",
        "prepare": "npm run compile",
        "pretest": "npm run compile",
        "posttest": "npm run lint",
        "coverage": "jest --coverage"
    },
    "files": [
        "dist"
    ],
    "devDependencies": {
        "@types/jest": "^29.5.14",
        "@types/node": "^22.7.5",
        "gts": "^6.0.2",
        "jest": "^29.7.0",
        "prettier": "^3.5.3",
        "ts-jest": "^29.3.3",
        "ts-node": "^10.9.2",
        "typescript": "^5.1.6"
    }
